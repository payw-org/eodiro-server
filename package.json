{
  "name": "eodiro-api2",
  "version": "2.0.0-beta.0",
  "client-version": "3.5.1",
  "main": "src/main.ts",
  "repository": "https://github.com/paywteam/eodiro-api2.git",
  "author": "PAYW",
  "license": "MIT",
  "scripts": {
    "sync-db:prod": "ts-node -r tsconfig-paths/register src/database/sync.ts",
    "sync-db:dev": "cross-env NODE_ENV=development ts-node -r tsconfig-paths/register src/database/sync.ts",
    "ts-node:prod": "ts-node -r tsconfig-paths/register",
    "ts-node:dev": "cross-env NODE_ENV=development ts-node -r tsconfig-paths/register",
    "test": "mocha -r ts-node/register -r tsconfig-paths/register \"./{,!(node_modules)/**}/*.spec.ts\"",
    "file-test:dev": "cross-env NODE_ENV=development mocha -r ts-node/register -r tsconfig-paths/register",
    "file-test": "mocha -r ts-node/register -r tsconfig-paths/register",
    "dev": "npm-run-all --parallel dev-ts type-check:watch codegen:watch",
    "dev-ts": "tsnd --no-notify --respawn --transpile-only -r tsconfig-paths/register src/main.ts",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch --preserveWatchOutput",
    "prebuild": "npm run clear && npm run generate",
    "build": "node fix-sequelize-type.js && ttsc",
    "start": "node --harmony build/src/main.js",
    "start:dev": "cross-env NODE_ENV=development node --harmony build/src/main.js",
    "pm2:start": "if pm2 show eodiro-api2 ; then npm run pm2:stop && npm run pm2:delete && npm run pm2:start ; else pm2 start build/src/main.js --name \"eodiro-api2\" ; fi",
    "pm2:restart": "if pm2 show eodiro-api2 ; then pm2 restart eodiro-api2 ; else echo 'No PM2 instance for eodiro API 2' ; fi",
    "pm2:stop": "if pm2 show eodiro-api2 ; then pm2 stop eodiro-api2 ; else echo 'No PM2 instance for eodiro API 2' ; fi",
    "pm2:delete": "if pm2 show eodiro-api2 ; then pm2 delete eodiro-api2 ; else echo 'No PM2 instance for eodiro API 2' ; fi",
    "pm2:reboot": "npm install && npm run build && npm run pm2:stop && npm run pm2:delete && npm run pm2:start",
    "clear": "rimraf build/*",
    "checkout-latest": "git reset --hard && git checkout $(git describe --tags `git rev-list --tags --max-count=1`)",
    "update": "git reset --hard && git fetch --prune && npm run checkout-latest && npm install && npm run pm2:reboot",
    "cdn:dev": "npm run ts-node:dev -- src/cdn/serve.ts",
    "cdn": "node --harmony build/src/cdn/serve.js",
    "pm2:cdn": "pm2 start npm --name 'eodiro-cdn' -- run 'cdn'",
    "one-api:refresh": "npm run --prefix src/api/one refresh",
    "one-api:deploy": "npm run one-api:refresh && npm run build && npm run --prefix src/api/one copy-packagejson && npm run --prefix build/src deploy",
    "introspect": "prisma introspect && node prisma/utils/map-and-pluralize",
    "generate": "prisma generate",
    "prisma:all": "npm run introspect && npm run generate",
    "codegen": "graphql-codegen",
    "codegen:watch": "graphql-codegen --watch"
  },
  "dependencies": {
    "@payw/cau-cafeteria-menus-scraper": "^2.1.4",
    "@payw/cau-timetable-scraper": "^2.1.1",
    "@prisma/client": "^2.0.0-beta.4",
    "apollo-server-express": "^2.13.0",
    "axios": "^0.19.2",
    "bcrypt": "^4.0.0",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "cron": "^1.8.2",
    "dayjs": "^1.8.26",
    "express": "^4.17.1",
    "feedsub": "^0.7.1",
    "graphql": "^14.6.0",
    "jsdom": "^16.2.2",
    "jwt-token": "^1.0.8",
    "lodash": "^4.17.15",
    "multer": "^1.4.2",
    "mustache": "^4.0.1",
    "mysql": "^2.18.1",
    "mysql2": "^2.1.0",
    "niceware": "^2.0.0",
    "nodemailer": "^6.4.6",
    "pluralize": "^8.0.0",
    "reflect-metadata": "^0.1.13",
    "rss-parser": "^3.8.0",
    "sequelize": "^6.0.0-beta.5",
    "serve-handler": "^6.1.2",
    "sql-formatter": "^2.3.3",
    "type-graphql": "^0.17.6",
    "uuid": "^7.0.3",
    "zombie": "^6.1.4"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^1.13.5",
    "@graphql-codegen/plugin-helpers": "^1.13.5",
    "@graphql-codegen/typescript": "^1.13.5",
    "@graphql-codegen/typescript-operations": "^1.13.5",
    "@graphql-codegen/typescript-resolvers": "^1.13.5",
    "@graphql-codegen/visitor-plugin-common": "^1.13.5",
    "@payw/cau-timetable-scraper-types": "^2.1.1",
    "@prisma/cli": "^2.0.0-beta.4",
    "@types/app-root-path": "^1.2.4",
    "@types/bcrypt": "^3.0.0",
    "@types/bluebird": "^3.5.30",
    "@types/chai": "^4.2.11",
    "@types/cors": "^2.8.6",
    "@types/cron": "^1.7.2",
    "@types/eslint": "^6.8.0",
    "@types/express": "^4.17.6",
    "@types/express-session": "^1.17.0",
    "@types/glob": "^7.1.1",
    "@types/graphql": "^14.5.0",
    "@types/jsdom": "^16.2.1",
    "@types/jsonwebtoken": "^8.3.9",
    "@types/lodash": "^4.14.150",
    "@types/mocha": "^7.0.2",
    "@types/multer": "^1.4.3",
    "@types/mustache": "^4.0.1",
    "@types/mysql": "^2.15.10",
    "@types/node": "^13.13.5",
    "@types/nodemailer": "^6.4.0",
    "@types/prettier": "^2.0.0",
    "@types/rimraf": "^3.0.0",
    "@types/serve-handler": "^6.1.0",
    "@types/uuid": "^7.0.3",
    "@types/validator": "^13.0.0",
    "@typescript-eslint/eslint-plugin": "^2.31.0",
    "@typescript-eslint/parser": "^2.31.0",
    "@zerollup/ts-transform-paths": "^1.7.17",
    "app-root-path": "^3.0.0",
    "chai": "^4.2.0",
    "chalk": "^4.0.0",
    "change-case": "^4.1.1",
    "cross-env": "^7.0.2",
    "dts-gen": "^0.6.0",
    "eslint": "^7.0.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "glob": "^7.1.6",
    "graphql-config": "^3.0.1",
    "graphql-import": "^1.0.2",
    "graphql-tools": "^5.0.0",
    "mocha": "^7.1.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "serve": "^11.3.0",
    "ts-node": "^8.10.1",
    "ts-node-dev": "^1.0.0-pre.44",
    "tsconfig-paths": "^3.9.0",
    "ttypescript": "^1.5.10",
    "typegraphql-prisma": "^0.2.1",
    "typescript": "^3.8.3",
    "yaml": "^1.9.2"
  }
}
