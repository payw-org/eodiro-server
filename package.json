{
  "name": "eodiro-api2",
  "version": "1.4.0",
  "main": "src/main.ts",
  "repository": "https://github.com/paywteam/eodiro-api2.git",
  "author": "PAYW",
  "license": "MIT",
  "scripts": {
    "sync-db:prod": "ts-node -r tsconfig-paths/register src/database/sync.ts",
    "sync-db:dev": "cross-env NODE_ENV=development ts-node -r tsconfig-paths/register src/database/sync.ts",
    "ts-node:prod": "ts-node -r tsconfig-paths/register",
    "ts-node:dev": "cross-env NODE_ENV=development ts-node -r tsconfig-paths/register",
    "seed-lectures": "ts-node -r tsconfig-paths/register src/direct/seed-lectures.ts",
    "test": "mocha -r ts-node/register -r tsconfig-paths/register ./**/*.spec.ts",
    "test-file": "mocha -r ts-node/register -r tsconfig-paths/register",
    "refresh-db": "node src/dev/before-dev/refresh-db.js",
    "symlink": "node --harmony src/dev/symlink.js",
    "dev": "npm-run-all --parallel dev-ts type-check:watch",
    "dev-ts": "tsnd --no-notify --respawn --transpile-only -r tsconfig-paths/register src/main.ts",
    "type-check:watch": "tsc --noEmit --watch",
    "prebuild": "npm run clear",
    "build": "node fix-sequelize-type.js && babel . --out-dir build --extensions '.ts,.js' && tsc --declaration --declarationDir build --emitDeclarationOnly",
    "start": "node --harmony build/src/main.js",
    "start:dev": "cross-env NODE_ENV=development node --harmony build/src/main.js",
    "pm2-start": "if pm2 show eodiro-api2 ; then npm run pm2-stop && npm run pm2-delete && npm run pm2-start ; else pm2 start build/src/main.js --name \"eodiro-api2\" ; fi",
    "pm2-restart": "if pm2 show eodiro-api2 ; then pm2 restart eodiro-api2 ; else echo 'No PM2 instance for eodiro API 2' ; fi",
    "pm2-stop": "if pm2 show eodiro-api2 ; then pm2 stop eodiro-api2 ; else echo 'No PM2 instance for eodiro API 2' ; fi",
    "pm2-delete": "if pm2 show eodiro-api2 ; then pm2 delete eodiro-api2 ; else echo 'No PM2 instance for eodiro API 2' ; fi",
    "pm2-reboot": "npm run build && npm run pm2-stop && npm run pm2-delete && npm run pm2-start",
    "clear": "rimraf build/*",
    "checkout-latest": "git reset --hard && git checkout $(git describe --tags `git rev-list --tags --max-count=1`)",
    "update": "git reset --hard && git fetch --prune && npm run checkout-latest && npm install && npm run pm2-reboot"
  },
  "_moduleAliases": {
    "@": "build"
  },
  "dependencies": {
    "@payw/cau-cafeteria-menus-scraper": "^2.1.3",
    "@payw/cau-timetable-scraper": "^2.0.2",
    "axios": "^0.19.2",
    "bcrypt": "^4.0.0",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "cron": "^1.8.2",
    "dayjs": "^1.8.23",
    "express": "^4.17.1",
    "express-session": "^1.17.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "multer": "^1.4.2",
    "mustache": "^4.0.1",
    "mysql": "^2.18.1",
    "mysql2": "^2.1.0",
    "niceware": "^2.0.0",
    "nodemailer": "^6.4.6",
    "sequelize": "^6.0.0-beta.5",
    "sql-formatter": "^2.3.3",
    "uuid": "^7.0.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "@babel/preset-typescript": "^7.9.0",
    "@types/app-root-path": "^1.2.4",
    "@types/bcrypt": "^3.0.0",
    "@types/bluebird": "^3.5.30",
    "@types/chai": "^4.2.11",
    "@types/cors": "^2.8.6",
    "@types/cron": "^1.7.2",
    "@types/eslint": "^6.1.8",
    "@types/express": "^4.17.3",
    "@types/express-session": "^1.17.0",
    "@types/glob": "^7.1.1",
    "@types/jsonwebtoken": "^8.3.8",
    "@types/lodash": "^4.14.149",
    "@types/mocha": "^7.0.2",
    "@types/multer": "^1.4.2",
    "@types/mustache": "^4.0.1",
    "@types/mysql": "^2.15.9",
    "@types/node": "^13.9.2",
    "@types/nodemailer": "^6.4.0",
    "@types/prettier": "^1.19.0",
    "@types/uuid": "^7.0.2",
    "@types/validator": "^12.0.1",
    "@typescript-eslint/eslint-plugin": "^2.24.0",
    "@typescript-eslint/parser": "^2.24.0",
    "app-root-path": "^3.0.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-plugin-transform-html-import-to-string": "0.0.1",
    "chai": "^4.2.0",
    "chalk": "^3.0.0",
    "change-case": "^4.1.1",
    "cross-env": "^7.0.2",
    "eslint": "^6.8.0",
    "glob": "^7.1.6",
    "mocha": "^7.1.1",
    "nodemon": "^2.0.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.2",
    "serve": "^11.3.0",
    "ts-node": "^8.7.0",
    "ts-node-dev": "^1.0.0-pre.44",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.8.3"
  }
}
