{
  "name": "eodiro-server",
  "version": "2.2.1",
  "client-version": "3.7.x",
  "main": "src/main.ts",
  "repository": "https://github.com/payw-org/eodiro-server.git",
  "author": "PAYW",
  "license": "MIT",
  "scripts": {
    "lint": "eslint . --ext .ts,.tsx",
    "sync-db:prod": "ts-node -r tsconfig-paths/register src/database/sync.ts",
    "sync-db:dev": "cross-env NODE_ENV=development ts-node -r tsconfig-paths/register src/database/sync.ts",
    "ts-node:prod": "ts-node -r tsconfig-paths/register",
    "ts-node:dev": "cross-env NODE_ENV=development ts-node -r tsconfig-paths/register",
    "test": "mocha -r ts-node/register -r tsconfig-paths/register \"./{,!(node_modules)/**}/*.spec.ts\"",
    "file-test:dev": "cross-env NODE_ENV=development mocha -r ts-node/register -r tsconfig-paths/register",
    "file-test": "mocha -r ts-node/register -r tsconfig-paths/register",
    "dev": "npm-run-all --parallel dev-ts type-check:watch one-api:refresh:watch cdn:dev",
    "dev-ts": "tsnd --no-notify --respawn --transpile-only -r tsconfig-paths/register src/main.ts",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch --preserveWatchOutput",
    "prebuild": "npm run clear && npm run generate",
    "build-fix-sequelize": "node fix-sequelize-type.js && ttsc",
    "build": "ttsc",
    "postbuild": "node prisma/utils/copy-schema-to-build",
    "start": "node --harmony build/src/main.js",
    "start:dev": "cross-env NODE_ENV=development node --harmony build/src/main.js",
    "pm2:start": "if pm2 show eodiro-server ; then npm run pm2:stop && npm run pm2:delete && npm run pm2:start ; else pm2 start build/src/main.js --name \"eodiro-server\" ; fi",
    "pm2:restart": "if pm2 show eodiro-server ; then pm2 restart eodiro-server ; else echo 'No PM2 instance for eodiro API 2' ; fi",
    "pm2:stop": "if pm2 show eodiro-server ; then pm2 stop eodiro-server ; else echo 'No PM2 instance for eodiro API 2' ; fi",
    "pm2:delete": "if pm2 show eodiro-server ; then pm2 delete eodiro-server ; else echo 'No PM2 instance for eodiro API 2' ; fi",
    "pm2:reboot": "npm install && npm run build && npm run pm2:stop && npm run pm2:delete && npm run pm2:start",
    "clear": "rimraf build/*",
    "checkout-latest": "git reset --hard && git checkout $(git describe --tags `git rev-list --tags --max-count=1`)",
    "update": "git reset --hard && git fetch --prune && npm run checkout-latest && npm install && npm run pm2:reboot",
    "cdn:dev": "npm run ts-node:dev -- src/cdn/serve.ts",
    "cdn": "node --harmony build/src/cdn/serve.js",
    "pm2:cdn": "pm2 start npm --name 'eodiro-cdn' -- run 'cdn'",
    "one-api:refresh:watch": "nodemon --watch src/api/one/scheme --exec \"npm run one-api:refresh\"",
    "one-api:refresh": "npm run --prefix src/api/one refresh",
    "one-api:deploy": "npm run one-api:refresh && npm run build && npm run --prefix src/api/one copy-packagejson && npm run --prefix build/src deploy",
    "one-api:deploy:next": "npm run one-api:refresh && npm run build && npm run --prefix src/api/one copy-packagejson && npm run --prefix build/src deploy:next",
    "introspect": "prisma introspect && node prisma/utils/map-and-pluralize",
    "generate": "prisma generate",
    "prisma:all": "npm run introspect && npm run generate"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run lint && npm run type-check"
    }
  },
  "dependencies": {
    "@payw/cau-cafeteria-menus-scraper": "^2.1.7",
    "@payw/cau-timetable-scraper": "^2.1.3",
    "@prisma/client": "^2.1.3",
    "axios": "^0.19.2",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "cron": "^1.8.2",
    "date-fns": "^2.14.0",
    "dayjs": "^1.8.28",
    "express": "^4.17.1",
    "jsdom": "^16.2.2",
    "jwt-token": "^1.0.9",
    "lodash": "^4.17.15",
    "multer": "^1.4.2",
    "mustache": "^4.0.1",
    "mysql": "^2.18.1",
    "mysql2": "^2.1.0",
    "nodemailer": "^6.4.10",
    "pluralize": "^8.0.0",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.2.3",
    "serve-handler": "^6.1.3",
    "socket.io": "^2.3.0",
    "sql-formatter": "^2.3.3",
    "uuid": "^8.2.0",
    "zombie": "^6.1.4"
  },
  "devDependencies": {
    "@payw/cau-timetable-scraper-types": "^2.1.1",
    "@prisma/cli": "^2.1.3",
    "@types/app-root-path": "^1.2.4",
    "@types/bcrypt": "^3.0.0",
    "@types/bluebird": "^3.5.32",
    "@types/chai": "^4.2.11",
    "@types/cors": "^2.8.6",
    "@types/cron": "^1.7.2",
    "@types/eslint": "^7.2.0",
    "@types/express": "^4.17.6",
    "@types/express-session": "^1.17.0",
    "@types/glob": "^7.1.2",
    "@types/jsdom": "^16.2.3",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/lodash": "^4.14.157",
    "@types/mocha": "^7.0.2",
    "@types/multer": "^1.4.3",
    "@types/mustache": "^4.0.1",
    "@types/mysql": "^2.15.14",
    "@types/node": "^14.0.14",
    "@types/nodemailer": "^6.4.0",
    "@types/prettier": "^2.0.1",
    "@types/puppeteer": "^3.0.0",
    "@types/rimraf": "^3.0.0",
    "@types/serve-handler": "^6.1.0",
    "@types/socket.io": "^2.1.8",
    "@types/uuid": "^8.0.0",
    "@types/validator": "^13.1.0",
    "@typescript-eslint/eslint-plugin": "^3.4.0",
    "@typescript-eslint/parser": "^3.4.0",
    "@zerollup/ts-transform-paths": "^1.7.18",
    "app-root-path": "^3.0.0",
    "chai": "^4.2.0",
    "chalk": "^4.1.0",
    "change-case": "^4.1.1",
    "cross-env": "^7.0.2",
    "eslint": "^7.3.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "glob": "^7.1.6",
    "husky": "^4.2.5",
    "mocha": "^8.0.1",
    "nodemon": "^2.0.4",
    "npm-run-all": "^4.1.5",
    "pending-xhr-puppeteer": "^2.2.0",
    "prettier": "^2.0.5",
    "puppeteer": "^3.3.0",
    "rimraf": "^3.0.2",
    "serve": "^11.3.2",
    "ts-node": "^8.10.2",
    "ts-node-dev": "^1.0.0-pre.49",
    "tsconfig-paths": "^3.9.0",
    "ttypescript": "^1.5.10",
    "typescript": "^3.9.5",
    "yaml": "^1.10.0"
  }
}
